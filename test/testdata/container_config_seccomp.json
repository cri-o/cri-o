{
	"metadata": {
		"name": "container1",
		"attempt": 1
	},
	"image": {
		"image": "redis:alpine"
	},
	"args": [
                "docker-entrypoint.sh",
                "redis-server"
	],
	"working_dir": "/data",
	"envs": [
		{
			"key": "PATH",
			"value": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
		},
		{
			"key": "TERM",
			"value": "xterm"
		},
		{
			"key": "TESTDIR",
			"value": "test/dir1"
		},
		{
			"key": "TESTFILE",
			"value": "test/file1"
		}
	],
	"labels": {
		"type": "small",
		"batch": "no"
	},
	"annotations": {
		"owner": "dragon",
		"daemon": "crio"
	},
	"privileged": true,
	"readonly_rootfs": true,
	"log_path": "",
	"stdin": false,
	"stdin_once": false,
	"tty": false,
	"linux": {
		"resources": {
			"cpu_period": 10000,
			"cpu_quota": 20000,
			"cpu_shares": 512,
			"oom_score_adj": 30
		},
		"security_context": {
			"seccomp_profile_path": "%VALUE%",
			"capabilities": {
				"add_capabilities": [
					"setuid",
					"setgid"
				],
				"drop_capabilities": [
				]
			},
			"selinux_options": {
				"user": "system_u",
				"role": "system_r",
				"type": "svirt_lxc_net_t",
				"level": "s0:c4,c5"
			}
		}
	}
}
