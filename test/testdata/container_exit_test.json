{
	"metadata": {
		"name": "podsandbox1-exit-test"
	},
	"image": {
		"image": "quay.io/crio/exit_test:latest"
	},
	"args": [
                "/exit_test"
	],
	"envs": [
		{
			"key": "PATH",
			"value": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
		}
	],
	"log_path": "",
	"stdin": false,
	"stdin_once": false,
	"tty": false,
	"linux": {
		"security_context": {
			"namespace_options": {
				"pid": 1
			},
			"readonly_rootfs": true
		}
	}
}
